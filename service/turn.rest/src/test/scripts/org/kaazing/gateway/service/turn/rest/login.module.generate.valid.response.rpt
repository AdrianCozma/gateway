connect http://localhost:8000/
connected

write method "GET"
write version "HTTP/1.1"
write header "Upgrade" "websocket"
write header "Connection" "Upgrade"
write header host
write header "Origin" "http://localhost:8000"
write parameter "service" "turn"
write header "Sec-WebSocket-Key" "nDaimG37f4nUqogPypithw=="
write header "Sec-WebSocket-Version" "13"
write header "Authorization" "Basic bWJ6cnhwZ2p5czo="
write notify BARRIER

read await BARRIER
read status "101" "Web Socket Protocol Handshake"
read version "HTTP/1.1"
read header "Connection" "Upgrade"
read header "Sec-WebSocket-Accept" /.*/
read header "Server" "Kaazing Gateway"
read header "Upgrade" "websocket"
read header "Content-Type" "application/json"
read header "Content-Length" /.*/

close
closed